<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".ResultActivity">

    <TextView
            android:text="Resultado"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/titulo_Resultado"/>
    <TextView
            android:text="Salário Bruto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/titulo_resultadoSalario"/>
    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/resultado_SalarioBruto"/>
    <TextView
            android:text="INSS"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/titulo_INSS"/>
    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/resultado_INSS"/>
    <TextView
            android:text="IRRF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/titulo_IRRF"/>
    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/resultado_IRRF"/>
    <TextView
            android:text="Outros Descontos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/titulo_resultadoOutro"/>
    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/resultado_OutrosDescontos"/>
    <TextView
            android:text="Salário Liquido"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/titulo_SalarioLiquido"/>
    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/resultado_SalarioLiquido"/>
    <TextView
            android:text="Descontos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/titulo_Descontos"/>
    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/resultado_Descontos"/>
</LinearLayout>